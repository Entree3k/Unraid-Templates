<?xml version="1.0"?>
<Container version="2">
  <Name>Edition-Manager</Name>
  <Repository>your-dockerhub-username/edition-manager</Repository>
  <Registry>https://hub.docker.com/r/your-dockerhub-username/edition-manager/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/your-github-username/edition-manager</Support>
  <Project>https://github.com/your-github-username/edition-manager</Project>
  <Overview>Edition Manager is a tool that automatically manages and updates movie edition information in your Plex library.</Overview>
  <Category>Tools: MediaApp:Video</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/Entree3k/Unraid-Templates/main/edition-manager/kQSCOtN6.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1689984000</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Config File" Target="/app/config.ini" Default="/mnt/user/appdata/edition-manager/config.ini" Mode="rw" Description="Configuration file location" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Metadata Backup Path" Target="/app/metadata_backup" Default="/mnt/user/appdata/edition-manager/metadata_backup" Mode="rw" Description="Metadata backup location" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="Timezone" Target="TZ" Default="America/Los_Angeles" Mode="" Description="Timezone (e.g., America/Los_Angeles)" Type="Variable" Display="always" Required="true" Mask="false"/>
  <Config Name="Run Mode" Target="RUN_MODE" Default="--all" Mode="" Description="Run mode (--all, --new, --reset, --backup, --restore)" Type="Variable" Display="always" Required="true" Mask="false"/>
</Container>
